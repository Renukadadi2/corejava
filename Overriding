class Animal 
{
public void move() { 	System.out.println("Animals are cute");
 }
 } 
class Cat extends Animal {
 public void move(){
System.out.println("Cats can walk and run");
  } 
  }
 public class TestCats {
 public static void main(String args[]) { 
Animal animal = new Animal(); 
 Animal b = new Cat();       
 animal.move();
 b.move(); 
 }
 }
